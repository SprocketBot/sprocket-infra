{
  "transport": {
    "url": "amqp://{{rmq.host}}:5672",
    "matchmaking_queue": "dev-matchmaking",
    "core_queue": "dev-core",
    "bot_queue": "dev-bot",
    "analytics_queue": "dev-analytics",
    "events_queue": "dev-events",
    "events_application_key": "dev-core",
    "queue": "{{celery.queue}}"
  },
  "gql": {
    "playground": false
  },
  "logger": {
    "levels": true
  },
  "minio": {
    "endPoint": "{{s3.endpoint}}",
    "port": {{ s3.port }},
    "useSSL": {{ s3.ssl }},
    "bucketNames": {
      "replays": "{{s3.bucket}}"
    }
  },
  "redis": {
    "port": {{redis.port}},
    "host": "{{ redis.host }}",
    "prefix": "{{ redis.prefix }}"
  },
  "db": {
    "host": "{{database.host}}",
    "port": {{database.port}},
    "username": "{{database.username}}",
    "database": "{{database.database}}",
    "enable_logs": true
  },
  "auth": {
    "discord": {
      "callbackUrl": "https://{{ api.url }}/login"
    },
    "google": {
      "callbackUrl": "https://{{ api.url }}/google/redirect"
    },
    "jwt_expiry": 12000,
    "frontend_callback": "https://{{ frontend.url }}/auth/callback"
  }
}
