{
  "celery": {
    "broker": "{{ celery.broker }}",
    "backend": "{{ celery.backend }}",
    "queue": "{{ celery.queue }}"
  },
  "transport": {
    "url": "amqp://{{ rmq.host }}:5672",
    "analytics_queue": "dev-analytics"
  },
  "minio": {
    "hostname": "{{ s3.endpoint }}",
    "bucket": "{{ s3.bucket }}",
    "parsed_object_prefix": "dev",
    "accessKey": "{{ s3.accessKey }}",
    "secure": true
  },
  "ballchasing": {
    "maxRetries": 4,
    "backoffFactor": 4
  },
  "tempDir": "/tmp",
  "parser": "ballchasing"
}
